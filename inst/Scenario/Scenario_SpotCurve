
    # This file is used to create the total return scenario objects
    # Spot rates are passed to the scenario analysis and the spot rate
    # curve is shifted according to object function

    Scenario(
    Name = "NCs",
    Type = "Immediate",
    ShiftType = "Parallel",
    Shiftbps = "0",
    Formula = function(RatesData = numeric(), Shiftbps = "character"){
    yield.basis = 100
    RatesData + (as.numeric(Shiftbps)/yield.basis)
    })

    Scenario(
    Name = "U25s",
    Type = "Immediate",
    ShiftType = "Parallel",
    Shiftbps = "25",
    Formula = function(RatesData = numeric(), Shiftbps = "character"){
    yield.basis = 100
    RatesData + (as.numeric(Shiftbps)/yield.basis)
    })

