
  # This file is used to create the total return scenario objects
  # Yield Curves are passed to the scenario analysis and the yield
  # curve is shifted according to object function
  
  Scenario(
  Name = "NC",
  Type = "Immediate",
  ShiftType = "Parallel",
  Shiftbps = "0",
  Formula = function(rates.data, Shiftbps){
  as.character(as.numeric(rates.data[1,2:length(rates.data)]) + as.numeric(Shiftbps)/yield.basis)
  })
  
  Scenario(
  Name = "D25",
  Type = "Immediate",
  ShiftType = "Parallel",
  Shiftbps = "-25",
  Formula = function(rates.data, Shiftbps){
  yield.basis = 100
  as.character(as.numeric(rates.data[1,2:length(rates.data)]) + as.numeric(Shiftbps)/yield.basis)
  })
  
  Scenario(
  Name = "D50",
